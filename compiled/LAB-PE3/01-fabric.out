#jinja2: lstrip_blocks: "True"
routing-options {
	router-id 10.1.1.3;
	autonomous-system 0.65535;
        dynamic-tunnels {
        dt-1 {
            rsvp-te rsvp-te-1 {
                label-switched-path-template {
                    default-template;
                }
                destination-networks {
                    10.1.1/24;
                }
            }
        }
    }
    }
#jinja2: lstrip_blocks: "True"
interfaces {
    	lo0 {
        unit 0 {
            description "PEERING INTERFACE --- DO NOT DELETE";
            family inet {
                address 10.1.1.3;
            }
                        family iso {
            	address 49.0000.0172.0016.0003.00;
            }
                    }
    }
    /* link_index: 8 | LAB-PE3 em1.0 169.254.0.8/31 --> 169.254.0.9/31 em3.0 LAB-COR3 */
	em1 {
		mtu 2000;
		description "WAN | LAB-COR3 em3";
		unit 0 {
            family inet {
				address 169.254.0.8/31;
			}
            family mpls;
            family iso;
            		}
	}
    /* link_index: 10 | LAB-PE3 em2.0 169.254.0.10/31 --> 169.254.0.11/31 em4.0 LAB-COR4 */
	em2 {
		mtu 2000;
		description "WAN | LAB-COR4 em4";
		unit 0 {
            family inet {
				address 169.254.0.10/31;
			}
            family mpls;
            family iso;
            		}
	}
    }
#jinja2: lstrip_blocks: "True"
protocols {
	rsvp {
        interface em1.0;
        interface em2.0;
        	}
	mpls {
        interface em1.0;
        interface em2.0;
            }
    ldp {
        interface em1.0;
        interface em2.0;
            }
    isis {
        interface em1.0 {
            level 2 {
                metric 5;
            }
        }
        interface em2.0 {
            level 2 {
                metric 5;
            }
        }
            }
}
