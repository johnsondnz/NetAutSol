
interfaces {
	lo0 {
        unit 0 {
            description "PEERING INTERFACE --- DO NOT DELETE";
            family inet {
                address 10.1.1.4;
            }
                        family iso {
            	address 49.0000.0172.0016.0004.00;
            }
                    }
    }
}
routing-options {
	router-id 10.1.1.4;
	autonomous-system 0.65535;
    dynamic-tunnels {
        dt-1 {
            rsvp-te rsvp-te-1 {
                label-switched-path-template {
                    default-template;
                }
                destination-networks {
                    10.1.1.0/24;
                }
            }
        }
    }
}


interfaces {
	/* link_index: 12 | LAB-PE4 em1.0 169.254.0.12/31 --> 169.254.0.13/31 em3.0 LAB-COR4 */
	em1 {
		mtu 2000;
		description "WAN | LAB-COR4 em3";
		unit 0 {
			family inet {
				address 169.254.0.12/31;
			}
			family mpls;
                        family iso;
            		}
	}
}
protocols {
	rsvp {
        interface em1.0;
	}
	mpls {
		        interface em1.0;
            }
    ldp {
    	interface em1.0;
        keepalive-timeout 180;
    }
        isis {
        interface em1.0 {
            point-to-point;
            level 1 disable;
            level 2 {
                metric 10;
                hello-interval 60;
            }
        }
    }
        }


